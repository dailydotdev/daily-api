{
  "name": "daily-api",
  "private": true,
  "scripts": {
    "prebuild": "rm -rf build",
    "build": "tsc",
    "postbuild": "cp -R ormconfig.js package.json package-lock.json seeds ./build",
    "cloud:init": "ts-node bin/initPubsub.ts",
    "db:migrate:create": "npm run typeorm -- migration:create -n",
    "db:migrate:latest": "npm run typeorm -- migration:run",
    "db:migrate:make": "npm run typeorm -- migration:generate -n",
    "db:migrate:reset": "npm run db:schema:drop && npm run db:migrate:latest",
    "db:migrate:rollback": "npm run typeorm -- migration:revert",
    "db:schema:drop": "npm run typeorm -- schema:drop",
    "db:schema:sync": "npm run typeorm -- schema:sync",
    "db:seed:export": "ts-node bin/export.ts",
    "db:seed:import": "ts-node bin/import.ts",
    "dev": "nodemon ./index.ts | pino-pretty",
    "dev:background": "MODE=background npm run dev",
    "lint": "eslint . --ext .js,.ts --max-warnings 0",
    "start": "node index.js | pino-stackdriver",
    "pretest": "npm run lint && cross-env NODE_ENV=test npm run db:migrate:reset",
    "test": "jest --testEnvironment=node --runInBand",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "@binxhealth/pino-stackdriver": "^3.0.0",
    "@dailydotdev/graphql-redis-subscriptions": "^2.4.3",
    "@dailydotdev/ts-ioredis-pool": "^1.0.2",
    "@fastify/cookie": "^8.3.0",
    "@fastify/cors": "^8.1.1",
    "@fastify/helmet": "^10.0.2",
    "@fastify/rate-limit": "^7.5.0",
    "@fastify/websocket": "^7.1.0",
    "@google-cloud/pubsub": "^3.1.0",
    "@google-cloud/trace-agent": "^5.1.6",
    "@graphql-tools/schema": "^8.5.0",
    "@mando75/typeorm-graphql-loader": "^0.3.0",
    "@sendgrid/mail": "^7.4.6",
    "@slack/webhook": "^6.0.0",
    "@types/shortid": "0.0.29",
    "apollo-server-errors": "^3.3.0",
    "apollo-server-types": "^3.6.2",
    "cloudinary": "^1.30.1",
    "date-fns": "^2.29.1",
    "date-fns-tz": "^1.3.6",
    "fast-json-stringify": "^2.7.10",
    "fastify": "^4.9.2",
    "fastify-plugin": "^4.3.0",
    "flagsmith-nodejs": "^1.1.0",
    "form-data": "^4.0.0",
    "graphql": "^16.3.0",
    "graphql-parse-resolve-info": "^4.12.3",
    "graphql-relay": "^0.10.0",
    "graphql-scalars": "^1.17.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-tools": "^8.3.6",
    "graphql-upload": "^15.0.1",
    "he": "^1.2.0",
    "highlight.js": "^11.6.0",
    "isbot": "^3.3.3",
    "jsonexport": "^3.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "markdown-it": "^13.0.1",
    "mercurius": "^11.2.0",
    "mercurius-cache": "^3.0.1",
    "mercurius-upload": "^5.0.0",
    "node-fetch": "^2.6.7",
    "pg": "^8.7.1",
    "pg-query-stream": "^4.2.1",
    "pino": "^8.3.0",
    "reflect-metadata": "^0.1.13",
    "rss": "^1.2.2",
    "shortid": "^2.2.16",
    "twit": "^2.2.11",
    "typeorm": "^0.2.45",
    "uuid": "^8.3.2",
    "validate.js": "^0.13.1"
  },
  "devDependencies": {
    "@fastify/static": "^5.0.2",
    "@types/algoliasearch": "^4.0.0",
    "@types/express": "^4.17.13",
    "@types/graphql": "^14.5.0",
    "@types/graphql-relay": "^0.7.0",
    "@types/ioredis": "^4.27.4",
    "@types/jest": "^28.1.6",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.11.45",
    "@types/node-fetch": "^2.6.2",
    "@types/rss": "0.0.29",
    "@types/sharp": "^0.30.4",
    "@types/supertest": "^2.0.11",
    "@types/twit": "^2.2.30",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "faker": "^5.5.3",
    "jest": "^28.1.3",
    "jest-junit": "^13.0.0",
    "jest-mock-extended": "^2.0.7",
    "mercurius-integration-testing": "^6.0.1",
    "nock": "^13.2.9",
    "nodemon": "^2.0.19",
    "pino-pretty": "^8.1.0",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
