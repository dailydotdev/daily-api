{
  "name": "daily-api",
  "private": true,
  "scripts": {
    "start": "node index.js | pino-stackdriver",
    "dev": "nodemon ./index.ts | pino-pretty",
    "lint": "eslint . --ext .js,.ts --max-warnings 0",
    "build": "tsc",
    "prebuild": "rm -rf build",
    "postbuild": "cp -R ormconfig.js package.json package-lock.json seeds ./build",
    "test": "jest --testEnvironment=node --runInBand",
    "pretest": "npm run lint && cross-env NODE_ENV=test npm run db:migrate:reset",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "db:schema:sync": "npm run typeorm -- schema:sync",
    "db:schema:drop": "npm run typeorm -- schema:drop",
    "db:migrate:create": "npm run typeorm -- migration:create -n",
    "db:migrate:make": "npm run typeorm -- migration:generate -n",
    "db:migrate:latest": "npm run typeorm -- migration:run",
    "db:migrate:rollback": "npm run typeorm -- migration:revert",
    "db:migrate:reset": "npm run db:schema:drop && npm run db:migrate:latest",
    "db:seed:import": "ts-node bin/import.ts",
    "db:seed:export": "ts-node bin/export.ts"
  },
  "dependencies": {
    "@amplitude/node": "^1.8.3",
    "@binxhealth/pino-stackdriver": "^3.0.0",
    "@google-cloud/pubsub": "^2.17.0",
    "@google-cloud/trace-agent": "^5.1.5",
    "@graphql-tools/schema": "^8.3.1",
    "@mando75/typeorm-graphql-loader": "^0.3.0",
    "@sendgrid/mail": "^7.4.6",
    "@slack/webhook": "^6.0.0",
    "@types/shortid": "0.0.29",
    "apollo-server-errors": "^3.3.0",
    "apollo-server-types": "^3.4.0",
    "cloudinary": "^1.27.0",
    "date-fns": "^2.24.0",
    "fast-json-stringify": "^2.7.10",
    "fastify": "^3.24.1",
    "fastify-cookie": "^5.4.0",
    "fastify-cors": "^6.0.2",
    "fastify-helmet": "^5.3.2",
    "fastify-plugin": "^3.0.0",
    "fastify-rate-limit": "^5.6.2",
    "fastify-websocket": "^4.0.0",
    "flagsmith-nodejs": "^1.1.0",
    "form-data": "^4.0.0",
    "graphql": "^15.7.2",
    "graphql-iso-date": "^3.6.1",
    "graphql-parse-resolve-info": "^4.12.0",
    "graphql-redis-subscriptions": "^2.4.0",
    "graphql-relay": "^0.9.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-tools": "^4.0.8",
    "he": "^1.2.0",
    "highlight.js": "^11.3.1",
    "ioredis": "^4.28.2",
    "isbot": "^3.3.3",
    "jsonexport": "^3.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "markdown-it": "^12.3.0",
    "mercurius": "^8.12.0",
    "mercurius-cache": "^0.11.0",
    "mercurius-upload": "^2.0.0",
    "node-fetch": "^2.6.6",
    "p-retry": "^4.6.1",
    "pg": "^8.7.1",
    "pg-query-stream": "^4.2.1",
    "pino": "^7.5.1",
    "reflect-metadata": "^0.1.13",
    "rss": "^1.2.2",
    "shortid": "^2.2.16",
    "twit": "^2.2.11",
    "typeorm": "^0.2.41",
    "uuid": "^8.3.2",
    "validate.js": "^0.13.1"
  },
  "devDependencies": {
    "@types/algoliasearch": "^4.0.0",
    "@types/express": "^4.17.13",
    "@types/graphql": "^14.5.0",
    "@types/graphql-relay": "^0.6.0",
    "@types/ioredis": "^4.27.4",
    "@types/jest": "^27.0.1",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.11.12",
    "@types/node-fetch": "^3.0.3",
    "@types/rss": "0.0.29",
    "@types/sharp": "^0.28.6",
    "@types/supertest": "^2.0.11",
    "@types/twit": "^2.2.30",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "faker": "^5.5.3",
    "fastify-static": "^4.5.0",
    "jest": "^27.4.4",
    "jest-junit": "^13.0.0",
    "jest-mock-extended": "^2.0.4",
    "mercurius-integration-testing": "^3.2.0",
    "nock": "^13.1.3",
    "nodemon": "^2.0.12",
    "pino-pretty": "^7.2.0",
    "prettier": "^2.4.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
}
