{
  "name": "daily-api",
  "private": true,
  "engines": {
    "node": "^12.19.0"
  },
  "scripts": {
    "start": "node index.js | pino-stackdriver",
    "dev": "nodemon ./index.ts | pino-pretty",
    "lint": "eslint . --ext .js,.ts --max-warnings 0",
    "build": "tsc",
    "prebuild": "rm -rf build",
    "postbuild": "cp -R ormconfig.js package.json package-lock.json seeds ./build",
    "test": "jest --testEnvironment=node --runInBand",
    "pretest": "npm run lint && cross-env NODE_ENV=test npm run db:migrate:reset",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "db:schema:sync": "npm run typeorm -- schema:sync",
    "db:schema:drop": "npm run typeorm -- schema:drop",
    "db:migrate:create": "npm run typeorm -- migration:create -n",
    "db:migrate:make": "npm run typeorm -- migration:generate -n",
    "db:migrate:latest": "npm run typeorm -- migration:run",
    "db:migrate:rollback": "npm run typeorm -- migration:revert",
    "db:migrate:reset": "npm run db:schema:drop && npm run db:migrate:latest",
    "db:seed:import": "ts-node bin/import.ts",
    "db:seed:export": "ts-node bin/export.ts"
  },
  "dependencies": {
    "@binxhealth/pino-stackdriver": "^3.0.0",
    "@google-cloud/profiler": "^4.1.2",
    "@google-cloud/pubsub": "^2.16.0",
    "@google-cloud/trace-agent": "^5.1.3",
    "@mando75/typeorm-graphql-loader": "^0.3.0",
    "@sendgrid/mail": "^7.4.5",
    "@slack/webhook": "^6.0.0",
    "@types/shortid": "0.0.29",
    "apollo-error-converter": "^1.1.1",
    "apollo-server-fastify": "^2.25.2",
    "apollo-server-plugin-response-cache": "^0.9.0",
    "cloudinary": "^1.26.2",
    "date-fns": "^2.22.1",
    "fast-json-stringify": "^2.7.7",
    "fastify": "^2.15.3",
    "fastify-cookie": "^3.6.1",
    "fastify-cors": "^3.0.3",
    "fastify-helmet": "^3.0.2",
    "fastify-plugin": "^1.6.1",
    "fastify-rate-limit": "^3.0.1",
    "feedparser": "^2.2.10",
    "graphql": "^14.7.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-parse-resolve-info": "^4.12.0",
    "graphql-redis-subscriptions": "^2.4.0",
    "graphql-relay": "^0.6.0",
    "graphql-subscriptions": "^1.2.1",
    "graphql-upload": "^12.0.0",
    "he": "^1.2.0",
    "ioredis": "^4.27.6",
    "isbot": "^3.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.1",
    "p-retry": "^4.6.1",
    "pg": "^8.6.0",
    "pg-query-stream": "^4.1.0",
    "reflect-metadata": "^0.1.13",
    "rss": "^1.2.2",
    "shortid": "^2.2.16",
    "twit": "^2.2.11",
    "typeorm": "0.2.32",
    "uuid": "^8.3.2",
    "validate.js": "^0.13.1"
  },
  "devDependencies": {
    "@types/algoliasearch": "^4.0.0",
    "@types/express": "^4.17.13",
    "@types/feedparser": "^2.2.4",
    "@types/graphql": "^14.5.0",
    "@types/graphql-relay": "^0.6.0",
    "@types/ioredis": "^4.26.5",
    "@types/jest": "^26.0.24",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/lodash": "^4.14.171",
    "@types/node": "^15.14.2",
    "@types/node-fetch": "^2.5.11",
    "@types/rss": "0.0.28",
    "@types/sharp": "^0.28.4",
    "@types/supertest": "^2.0.11",
    "@types/twit": "^2.2.30",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "apollo-server-testing": "^2.25.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "faker": "^5.5.3",
    "fastify-static": "^2.7.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.2.0",
    "jest-mock-extended": "^1.0.16",
    "nock": "^13.1.1",
    "nodemon": "^2.0.12",
    "pino-pretty": "^5.1.1",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  }
}
