{
  "name": "daily-api",
  "private": true,
  "scripts": {
    "prebuild": "rm -rf build",
    "build": "tsc",
    "postbuild": "cp -R ormconfig.js package.json pnpm-lock.yaml seeds ./build",
    "cli": "node bin/cli",
    "db:migrate:create": "pnpm run typeorm migration:create",
    "db:migrate:latest": "pnpm run typeorm migration:run -d src/data-source.ts",
    "db:migrate:make": "pnpm run typeorm migration:generate -d src/data-source.ts",
    "db:migrate:reset": "pnpm run db:schema:drop && pnpm run db:migrate:latest",
    "db:migrate:rollback": "pnpm run typeorm migration:revert -d src/data-source.ts",
    "db:schema:drop": "pnpm run typeorm schema:drop -d src/data-source.ts",
    "db:schema:sync": "pnpm run typeorm schema:sync",
    "db:seed:export": "ts-node bin/export.ts",
    "db:seed:import": "ts-node bin/import.ts",
    "dev": "nodemon ./bin/cli api",
    "dev:background": "nodemon ./bin/cli background",
    "dev:personalized-digest": "nodemon ./bin/cli personalized-digest",
    "dev:temporal-worker": "nodemon ./src/temporal/local.ts",
    "dev:temporal-server": "temporal server start-dev",
    "lint": "eslint . --ext .js,.ts --max-warnings 0",
    "start": "pnpm run cli api",
    "start:background": "pnpm run cli background",
    "pretest": "cross-env NODE_ENV=test pnpm run db:migrate:reset",
    "test": "jest --testEnvironment=node --runInBand",
    "typeorm": "typeorm-ts-node-commonjs",
    "prepare": "corepack enable || true"
  },
